{
  "name": "zazu-giphy",
  "version": "1.0.8",
  "description": "Gif giving is the greatest love language of all.",
  "blocks": {
    "input": [
      {
        "id": "searcher",
        "type": "PrefixScript",
        "prefix": "giphy",
        "space": true,
        "args": "required",
        "script": "index.js",
        "connections": [
          "Copy"
        ]
      },
      {
        "id": "help",
        "type": "PrefixScript",
        "prefix": "commands",
        "space": true,
        "args": "required",
        "script": "commands.js",
        "connections": [
          "Copy"
        ]
      },
      {
        "id": "commands",
        "type": "PrefixScript",
        "prefix": "help",
        "space": false,
        "args": "required",
        "script": "help.js",
        "connections": [
          "Copy"
        ]
      }
    ],
    "output": [
      {
        "id": "Copy",
        "type": "CopyToClipboard",
        "text": "{value}"
      }
    ]
  }
}
